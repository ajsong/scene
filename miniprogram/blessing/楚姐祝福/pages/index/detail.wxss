@import "../../newyear.wxss";
.detail{padding-bottom:93rpx;}

.detail .bannerBox, .detail .footerBox{height:auto; text-align:initial;}
.detail .banner-fixed, .detail .footer-fixed{position:sticky; left:0; right:0; top:0; z-index:11; background:#fff; -webkit-transition:all 0.3s ease-out; transition:all 0.3s ease-out;}
