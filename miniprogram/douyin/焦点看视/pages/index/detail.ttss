@import "../../newyear.ttss";
.detail{padding-bottom:93rpx;}
.detail .bannerBox, .detail .footerBox{height:auto; text-align:initial;}
/* .detail .videoAdMinHeight{min-height:571rpx;} */
.detail .banner-fixed, .detail .footer-fixed{position:sticky; left:0; right:0; top:0; z-index:11; background:#fff; -webkit-transition:all 0.3s ease-out; transition:all 0.3s ease-out;}
.detail .banner, .detail .footer{margin:0 28rpx; border-radius:11rpx; height:auto; background:no-repeat center center; background-size:cover;}
.detail .footerBox{margin-bottom:28rpx;}
.detail .bannerBox > view, .detail .bannerBox > button, .detail .footerBox > view, .detail .footerBox > button{height:234rpx;}
.detail .title{text-align:left; padding:28rpx; padding-bottom:0; font-size:37rpx; font-weight:700;}
.detail .time{text-align:left; padding:0 28rpx; color:#999; line-height:70rpx; font-size:35rpx;}
.detail .time view{display:block; float:right; padding:0 35rpx; height:100%; text-decoration:none; font-size:28rpx; border-radius:9rpx; color:#fff; background:#ff9902;}
.detail .time .mp-title{color:#576b95; margin-right:23rpx;}
.detail .mp-img{height:93rpx; margin:0 28rpx; margin-top:23rpx; background:url(data:image/gif;base64,) no-repeat left center; background-size:contain; /*-webkit-animation:mp-spin 0.5s steps(12, end) infinite; animation:mp-spin 0.5s steps(12, end) infinite;*/}
@-webkit-keyframes mp-spin{0%{-webkit-transform:translateY(0);}50%{-webkit-transform:translateY(-18rpx);}100%{-webkit-transform:translateY(0);}}
@keyframes mp-spin{0%{transform:translateY(0);}50%{transform:translateY(-18rpx);}100%{transform:translateY(0);}}
.detail .subscribe-img{height:79rpx; background:url(data:image/png;base64,) no-repeat left center; background-size:contain; /*-webkit-animation:subscribe-spin 0.5s steps(12, end) infinite; animation:subscribe-spin 0.5s steps(12, end) infinite;*/}
@-webkit-keyframes subscribe-spin{0%{-webkit-transform:translateY(0);}50%{-webkit-transform:translateY(-11rpx);}100%{-webkit-transform:translateY(0);}}
@keyframes subscribe-spin{0%{transform:translateY(0);}50%{transform:translateY(-11rpx);}100%{transform:translateY(0);}}
.detail .trans{height:187rpx; text-align:left; margin:35rpx 28rpx; padding-left:159rpx; padding-right:191rpx; border-radius:16rpx; background:#fff; box-shadow:0 0 28rpx rgba(0,0,0,0.3); overflow:hidden;}
.detail .trans .btn{float:right; width:135rpx; height:79rpx; line-height:79rpx; margin-top:54rpx; margin-right:-163rpx; border-radius:9rpx; background:#0B7114; color:#fff; font-size:39rpx; text-align:center; -webkit-animation:trans-spin 0.3s steps(3, end) infinite; animation:trans-spin 0.3s steps(3, end) infinite;}
.detail .trans .pic{float:left; width:131rpx; height:100%; margin-left:-159rpx; background:no-repeat center center; background-size:cover;}
.detail .trans .tit{height:56rpx; line-height:56rpx; font-size:32rpx; color:#ab1942; margin-top:37rpx;}
.detail .trans .tip{height:56rpx; line-height:56rpx; font-size:30rpx; color:#576b95;}
.detail .trans .tip text{color:#f00;}
@-webkit-keyframes trans-spin{0%{color:#fff;}50%{color:transparent;}100%{color:#fff;}}
@keyframes trans-spin{0%{color:#fff;}50%{color:transparent;}100%{color:#fff;}}
.detail .content{text-align:left; padding:28rpx; font-size:32rpx;}
.detail .content rich-text{text-align:left; font-size:32rpx; width:100%; overflow:hidden; float:left;}
