/* 密码框 */
.passwordView{position:relative;}
.passwordView > view{position:relative; width:100%; height:100%;}
.passwordView .inp{position:absolute; left:0; top:0; z-index:1; opacity:0; width:100%; height:100%; border:0; background-color:transparent;}
.passwordView .font{display:block; position:absolute; left:0; top:0; z-index:1; width:2rpx; height:100%; padding:28rpx 0; box-sizing:border-box; -webkit-transition:opacity 300ms ease-out; transition:opacity 300ms ease-out;}
.passwordView .font:after{content:''; display:block; width:100%; height:100%; background:#00a0e9; box-shadow:0 0 11rpx rgba(0,160,233,0.5); -webkit-animation:font-opacity 1s steps(10, end) infinite; animation:font-opacity 1s steps(10, end) infinite;}
.passwordView .ul{list-style:none; padding:0; margin:0; width:100%; height:100%; box-sizing:border-box; overflow:hidden; -webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}
.passwordView .ul:after{content:''; display:block; width:100%; clear:both;}
.passwordView .ul .li{position:relative; float:left; padding:0; margin:0; box-sizing:border-box;}
.passwordView .ul .li text{display:block; position:absolute; left:50%; top:50%; transform:translate(-50%,-50%);}
.passwordView .ul .li input{width:100%; height:100%; padding:0; margin:0; text-align:center; color:#333; font-size:42rpx; background-color:transparent; border:none; box-sizing:border-box; -webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}
@-webkit-keyframes font-opacity{50%{opacity:0;}100%{opacity:1;}}
@keyframes font-opacity{50%{opacity:0;}100%{opacity:1;}}
.passwordView .ring .ul{border:2rpx solid #999; border-radius:7rpx; background:#fff;}
.passwordView .ring .inp:focus + .ul{border:2rpx solid #00a0e9; box-shadow:0 0 11rpx rgba(0,160,233,0.5);}
.passwordView .ring .error + .ul{border:2rpx solid #ff6464 !important; box-shadow:0 0 11rpx rgba(255,100,100,0.5) !important;}
.passwordView .ring .ul .li{border-right:2rpx solid #e5e5e5;}
.passwordView .ring .ul .li:last-child{border:none;}
.passwordView .underline .ul .li text{padding:0 14rpx;}
.passwordView .underline .ul .li input{border-bottom:2rpx solid #ccc;}
.passwordView .underline .ul .li input.this{border-bottom:2rpx solid #00a0e9;}
.passwordView .underline .error + .ul .li input{border-bottom:2rpx solid #ce0000;}

/* toast提示框 */
.toast-view{position:fixed; top:50%; left:50%; z-index:901; transform:translate(-50%,-50%); height:65rpx; line-height:65rpx; padding:0 35rpx; font-size:28rpx; border-radius:9rpx; color:#fff; background:rgba(51,51,51,0.8); -webkit-backdrop-filter:blur(35rpx); backdrop-filter:blur(35rpx); transition:all 300ms ease-out;}
.toast-scale-in{transform:translate(-50%,-50%) scale(1.2); opacity:0;}
.toast-scale-out{transform:translate(-50%,-50%) scale(0.8); opacity:0;}

/* 自定义内容弹框 */
.dialog-bg{position:fixed; top:0; left:0; z-index:900; width:100%; height:100%; background:rgba(0,0,0,0.5);}
.dialog-view{position:fixed; top:50%; left: 50%; z-index:901; width:609rpx; height:auto; overflow:hidden; background:#fff; border-radius:11rpx; text-align:center; transform:translate(-50%,-50%); transition:all 300ms ease-out;}
.dialog-view .dialog-title{position:relative; display:block; height:70rpx; line-height:70rpx; color:#ccc; font-size:28rpx;}
.dialog-view .dialog-content{padding:23rpx 46rpx; height:auto; font-size:32rpx; text-align:left;}
.dialog-view .dialog-content:after{content:""; display:block; clear:both; height:0; overflow:hidden;}
.dialog-view .dialog-title + .dialog-content{padding-top:23rpx;}
.dialog-view .dialog-btns{position:relative; height:93rpx;}
.dialog-view .dialog-btns .dialog-btn{position:relative; float:left; height:100%;}
.dialog-view .dialog-btns .dialog-btn button{width:100%; height:100%; line-height:93rpx; font-size:32rpx; color:#fff; background:#ccc; border-radius:0; border:none; padding:0; margin:0;}
.dialog-view .dialog-btns .dialog-btn.ok button, .dialog-view .dialog-btns .dialog-btn.blue button{background:#00a0e9;}
.dialog-view .dialog-btns .dialog-btn.green button,
.dialog-view .dialog-btns .dialog-btn.wxauth button{background:#3cc51f;}
.dialog-view.wx .dialog-btns:before,
.dialog-view.dialog-wxauth .dialog-btns:before{content:""; display:block; position:absolute; left:0; top:0; z-index:1; background:#d5d5d5; width:100%; height:2rpx; transform:scaleY(0.5); transform-origin:top;}
.dialog-view.wx .dialog-btns .dialog-btn button,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn button{color:#000; background-color:transparent;}
.dialog-view.wx .dialog-btns .dialog-btn:nth-child(n+2):after,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn:nth-child(n+2):after{content:""; display:block; position:absolute; left:0; top:0; z-index:1; background:#d5d5d5; width:2rpx; height:100%; transform:scaleX(0.5); transform-origin:left;}
.dialog-view.wx .dialog-btns .dialog-btn.ok button,
.dialog-view.wx .dialog-btns .dialog-btn.green button,
.dialog-view.wx .dialog-btns .dialog-btn.wxauth button,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn.ok button,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn.green button,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn.wxauth button{color:#3cc51f;}
.dialog-view.wx .dialog-btns .dialog-btn.blue button,
.dialog-view.dialog-wxauth .dialog-btns .dialog-btn.blue button{color:#00a0e9;}
.dialog-view.dialog-wxauth .dialog-title{height:89rpx; line-height:89rpx; font-weight:bold; color:#333; font-size:32rpx;}
.dialog-view.dialog-wxauth .dialog-title:after{content:""; display:block; position:absolute; left:0; bottom:0; z-index:1; background:#d5d5d5; width:100%; height:2rpx; transform:scaleY(0.5); transform-origin:bottom;}
.dialog-fade{opacity:0;}
.dialog-down{transform:translate(-50%,calc(-50% + 117rpx)); opacity:0;}
.dialog-up{transform:translate(-50%,calc(-50% - 117rpx)); opacity:0;}
.dialog-scale-in{transform:translate(-50%,-50%) scale(1.2); opacity:0;}
.dialog-scale-out{transform:translate(-50%,-50%) scale(0.8); opacity:0;}

/* 登录授权界面 */
.login-auth{position:fixed; z-index:999999; left:0; top:0; width:100%; height:100%; -webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}

.login-auth.wxauth{-webkit-transform:translateY(100%); transform:translateY(100%);}
.login-auth-in.wxauth{-webkit-transform:translateY(0); transform:translateY(0);}
.login-auth-out.wxauth{-webkit-transform:translateY(100%); transform:translateY(100%);}
.login-auth.wxauth > view{width:100%; height:100%; overflow:hidden; background:#fff;}
.login-auth.wxauth > view label{display:block; width:187rpx; height:187rpx; margin:0 auto; margin-top:175rpx; background:url(http://img.softstao.com/uploadfiles/client/1/pic/2018/06/28/1862815200347474.png) no-repeat center center; background-size:cover;}
.login-auth.wxauth > view text{display:block; margin:0 117rpx; margin-top:46rpx; font-size:32rpx; text-align:left; color:#333;}
.login-auth.wxauth > view button{position:absolute; z-index:1; left:50%; bottom:175rpx; width:304rpx; height:70rpx; line-height:70rpx; font-size:30rpx; -webkit-transform:translateX(-50%); transform:translateX(-50%); color:#3cc51f; border:2rpx solid #3cc51f; border-radius:9rpx; box-sizing:border-box;}

.login-auth.image-wxauth{background:rgba(0,0,0,0.6); opacity:0;}
.login-auth-in.image-wxauth{opacity:1;}
.login-auth-out.image-wxauth{opacity:0;}
.login-auth.image-wxauth > view{position:absolute; left:50%; top:50%; overflow:hidden; width:637rpx; height:789rpx; -webkit-transform:translate(-50%,-50%); transform:translate(-50%,-50%); border-radius:23rpx; background:#fefefe;}
.login-auth.image-wxauth > view view{height:117rpx; line-height:117rpx; font-size:32rpx;}
.login-auth.image-wxauth > view text{display:block; padding-top:28rpx; line-height:39rpx; font-size:30rpx;}
.login-auth.image-wxauth > view label{display:block; width:503rpx; height:440rpx; margin:0 auto; background:url(http://img.softstao.com/uploadfiles/pic/2019/01/13/1911314005019757.png) no-repeat center center; background-size:cover;}
.login-auth.image-wxauth > view button{width:503rpx; height:79rpx; line-height:79rpx; margin:0 auto; margin-top:23rpx; background:#3cc51f; color:#fff; font-size:35rpx; border-radius:39rpx;}

