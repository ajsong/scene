page, view, text, label, input, textarea, button, navigator{font-size:inherit; margin:0 auto; color:inherit;}
page{position:relative; display:block; color:#333; font-size:28rpx; text-align:center; min-height:100%; background:#fff; -webkit-font-smoothing:antialiased;}
page:after{content:""; display:none; position:fixed; z-index:999999; left:0; top:0; width:100%; height:1rpx; overflow:hidden; background:#e5e5e5;}
button{padding:0;}
button:after{display:none;}
input, textarea{text-align:left;}
textarea{line-height:1;}
.overflow-scrolling{-webkit-overflow-scrolling:touch;}
.swiper{width:100%; height:358rpx;}
.swiper swiper{width:100%; height:100%;}
swiper navigator, swiper view, swiper video{position:relative; background:no-repeat center center; background-size:cover; width:100%; height:100%;}
swiper text{display:block; position:absolute; left:0; right:0; top:0; text-align:left; color:#fff; padding:28rpx; font-size:32rpx; background:rgba(0,0,0,0.4);}

.gr{background-color:#f3f3f3;}
.hidden{display:none !important;}
.blank{clear:both; display:block; overflow:hidden; width:auto !important; height:23rpx !important; font-size:0; margin:0 !important; padding:0 !important;}
.clear{clear:both; display:block; overflow:hidden; width:auto !important; height:0 !important; font-size:0; margin:0 !important; padding:0 !important;}
.clear-before:before{content:""; display:block; clear:both; overflow:hidden; width:auto !important; height:0 !important; font-size:0; margin:0 !important; padding:0 !important;}
.clear-after:after{content:""; display:block; clear:both; overflow:hidden; width:auto !important; height:0 !important; font-size:0; margin:0 !important; padding:0 !important;}
.placeholder{color:#cdcdcd !important;}
.nopic{background-image:url(http://bangfang.b0.upaiyun.com/uploadfiles/pic/2018/01/16/1811622361129565.png);}
.norecord{position:absolute; z-index:1; left:50%; top:50%; transform:translate(-50%,-50%); width:100%; height:281rpx; line-height:281rpx; text-align:center; font-size:28rpx; color:#999;}
.height-wrap{height:100%; box-sizing:border-box;}
.b, .strong{font-weight:700;}
.i{font-style:italic;}
.s{text-decoration:line-through;}
.h1{display:block; font-size:2em; margin-block-start:0.67em; margin-block-end:0.67em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.h2{display:block; font-size:1.5em; margin-block-start:0.83em; margin-block-end:0.83em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.h3{display:block; font-size:1.17em; margin-block-start:1em; margin-block-end:1em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.h4{display:block; margin-block-start:1.33em; margin-block-end:1.33em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.h5{display:block; font-size:0.83em; margin-block-start:1.67em; margin-block-end:1.67em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.h6{display:block; font-size:0.67em; margin-block-start:2.33em; margin-block-end:2.33em; margin-inline-start:0px; margin-inline-end:0px; font-weight:bold;}
.pullRefresh{position:relative; z-index:2; width:100%; height:100%; overflow:auto; -webkit-overflow-scrolling:touch;}

.toolBar{background:rgba(245,245,245,0.97); -webkit-backdrop-filter:blur(35rpx);}
.toolBar-black{background:rgba(88,88,88,0.97); -webkit-backdrop-filter:blur(35rpx);}

/* 角标 */
.badge{position:relative;}
.badge .sub{display:block; position:absolute; z-index:10; top:11rpx; right:0; padding:0 7rpx; text-align:center; min-width:32rpx; height:32rpx; line-height:32rpx; border-radius:16rpx; background:#f00; box-sizing:border-box;}
.badge .sub:empty{display:none;}
.badge .sub .b{display:block; margin:0 auto; height:100%; font-weight:normal; color:#fff; font-size:28rpx; transform:scale(0.66);}

/* 分割线 */
.ge-top, .ge-bottom, .ge-left, .ge-right{position:relative;}
.ge-top:before, .ge-bottom:after, .ge-left:before, .ge-right:after{content:""; display:block; position:absolute; z-index:10; background:#c7c7c7;}
.ge-top:before, .ge-bottom:after{left:0; width:100%; height:2rpx; -webkit-transform:scaleY(0.5); transform:scaleY(0.5);}
.ge-left:before, .ge-right:after{top:0; width:2rpx; height:100%; -webkit-transform:scaleX(0.5); transform:scaleX(0.5);}
.ge-top:before{top:0; -webkit-transform-origin:top; transform-origin:top;}
.ge-bottom:after{bottom:0; -webkit-transform-origin:bottom; transform-origin:bottom;}
.ge-left:before{left:0; -webkit-transform-origin:left; transform-origin:left;}
.ge-right:after{right:0; -webkit-transform-origin:right; transform-origin:right;}
.ge-light:before, .ge-light:after{background:#eaeaea;}

/* 缩放动画 */
.scale-animate{-webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}
.scale-animate-0{-webkit-transform:scale(0); transform:scale(0);}

/* 圆角 */
.ra-top{-moz-border-radius-topleft:9rpx; -moz-border-radius-topright:9rpx; -webkit-border-top-left-radius:9rpx; -webkit-border-top-right-radius:9rpx; border-top-left-radius:9rpx; border-top-right-radius:9rpx;}
.ra-bottom{-moz-border-radius-bottomleft:9rpx; -moz-border-radius-bottomright:9rpx; -webkit-border-bottom-left-radius:9rpx; -webkit-border-bottom-right-radius:9rpx; border-bottom-left-radius:9rpx; border-bottom-right-radius:9rpx;}
.ra-left{-moz-border-radius-topleft:9rpx; -moz-border-radius-bottomleft:9rpx; -webkit-border-top-left-radius:9rpx; -webkit-border-bottom-left-radius:9rpx; border-top-left-radius:9rpx; border-bottom-left-radius:9rpx;}
.ra-right{-moz-border-radius-topright:9rpx; -moz-border-radius-bottomright:9rpx; -webkit-border-top-right-radius:9rpx; -webkit-border-bottom-right-radius:9rpx; border-top-right-radius:9rpx; border-bottom-right-radius:9rpx;}
.ra-left-top{-moz-border-radius-topleft:9rpx; -webkit-border-top-left-radius:9rpx; border-top-left-radius:9rpx;}
.ra-right-top{-moz-border-radius-topright:9rpx; -webkit-border-top-right-radius:9rpx; border-top-right-radius:9rpx;}
.ra-left-bottom{-moz-border-radius-bottomleft:9rpx; -webkit-border-bottom-left-radius:9rpx; border-bottom-left-radius:9rpx;}
.ra-right-bottom{-moz-border-radius-bottomright:9rpx; -webkit-border-bottom-right-radius:9rpx; border-bottom-right-radius:9rpx;}

/* 底部按钮 */
.submitView, .buttonView, .positionView{text-align:center; left:0; bottom:0; margin:0 auto; padding:23rpx; width:100%; height:auto; overflow:hidden; box-sizing:border-box;}
.positionView{padding:0;}
.submitView:before, .buttonView:before, .positionView:before{content:""; clear:both; display:block; overflow:hidden; width:auto; height:0; font-size:0;}
.submitView, .positionView{position:fixed; z-index:100; background:none;}
.submitView view, .buttonView view, .positionView view,
.submitView navigator, .buttonView navigator, .positionView navigator,
.submitView button, .buttonView button, .positionView button{display:block; text-decoration:none; border:none; padding:0; margin:0; margin-bottom:23rpx; background:#fff; color:#333; font-size:30rpx; text-align:center; float:left; width:100%; height:93rpx; line-height:93rpx; overflow:hidden; border-radius:7rpx; -webkit-appearance:none; -webkit-transition:background 200ms ease-out; transition:background 200ms ease-out;}
.positionView view, .positionView navigator, .positionView button{border-radius:0; height:103rpx; line-height:103rpx;}
.submitView view:last-child, .buttonView view:last-child, .positionView view:last-child,
.submitView navigator:last-child, .buttonView navigator:last-child, .positionView navigator:last-child,
.submitView button:last-child, .buttonView button:last-child, .positionView button:last-child{margin-bottom:0;}
.submitView .btn, .buttonView .btn, .positionView .btn{background:#ce0000; color:#fff;}
.submitView .disabled, .buttonView .disabled, .positionView .disabled{background:#ccc; color:#ddd; -webkit-pointer-events:none; pointer-events:none;}
.gr .submitView, .gr .positionView{background:rgba(242,242,242,0.97);}
.submitView.btnView-2 view, .buttonView.btnView-2 view,
.submitView.btnView-2 navigator, .buttonView.btnView-2 navigator,
.submitView.btnView-2 button, .buttonView.btnView-2 button{width:calc((100% - 23rpx) / 2); margin-bottom:0;}
.submitView.btnView-2 view:not(:first-child), .buttonView.btnView-2 view:not(:first-child),
.submitView.btnView-2 navigator:not(:first-child), .buttonView.btnView-2 navigator:not(:first-child),
.submitView.btnView-2 button:not(:first-child), .buttonView.btnView-2 button:not(:first-child){margin-left:23rpx;}
.positionView.btnView-2 view, .positionView.btnView-2 navigator, .positionView.btnView-2 button{width:calc(100% / 2); margin-bottom:0;}
.submitView.btnView-3 view, .buttonView.btnView-3 view,
.submitView.btnView-3 navigator, .buttonView.btnView-3 navigator,
.submitView.btnView-3 button, .buttonView.btnView-3 button{width:calc((100% - 46rpx) / 3); margin-bottom:0; font-size:30rpx;}
.submitView.btnView-3 view:not(:first-child), .buttonView.btnView-3 view:not(:first-child),
.submitView.btnView-3 navigator:not(:first-child), .buttonView.btnView-3 navigator:not(:first-child),
.submitView.btnView-3 button:not(:first-child), .buttonView.btnView-3 button:not(:first-child){margin-left:23rpx;}
.positionView.btnView-3 view, .positionView.btnView-3 navigator, .positionView.btnView-3 button{width:calc(100% / 3); margin-bottom:0; font-size:30rpx;}

/* push箭头 */
.push-ico{padding-right:82rpx !important; box-sizing:border-box; background-repeat:no-repeat; background-image:url("data:image/svg+xml;charset=utf-8,%3Csvg%20viewBox%3D'0%200%2060%20120'%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%3E%3Cpath%20d%3D'm60%2061.5-38.25%2038.25-9.75-9.75%2029.25-28.5-29.25-28.5%209.75-9.75z'%20fill%3D'%23c7c7cc'%2F%3E%3C%2Fsvg%3E"); background-position:-webkit-calc(100% - 35rpx) center; background-position:calc(100% - 35rpx) center; background-size:23rpx 46rpx;}

/* 表格 */
.tableView{width:100%; height:auto;}
.tableView > .li, .tableView .section > .li{position:relative; z-index:2; min-height:103rpx; background-color:#fff; box-sizing:border-box; -webkit-transform:translateX(0); transform:translateX(0); -webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}
.tableView .li > view:first-child{box-sizing:border-box;}
.tableView .section{position:relative; width:100%; height:auto;}
.tableView .section:before{content:""; position:absolute; left:0; top:-3rpx; z-index:3; display:block; width:100%; height:2rpx; background:#c7c7c7; -webkit-transform-origin:top; transform-origin:top; -webkit-transform:scaleY(0.5); transform:scaleY(0.5);}
.tableView .section > .header{position:-webkit-sticky; position:sticky; top:0; z-index:3; margin:0; background:rgba(240,240,240,0.97); color:#333; font-weight:bold; width:100%; height:51rpx; line-height:51rpx; overflow:hidden; clear:both; text-align:left; white-space:nowrap; text-overflow:ellipsis; box-sizing:border-box;}
.tableView navigator{display:block; text-decoration:none;}
.tableView .h1{position:relative; text-align:left; margin:0; padding:0; margin-left:35rpx; padding-right:35rpx; font-size:32rpx; min-height:103rpx; line-height:103rpx; overflow:hidden; font-weight:normal; -webkit-transition:all 300ms ease-out; transition:all 300ms ease-out;}
.tableView-noMargin .h1, .tableView .h1.noMargin{margin:0; padding-left:35rpx;}
.tableView > .li:last-child:after, .tableView .h1:after{content:""; position:absolute; z-index:3; left:0; bottom:0; display:block; width:100%; height:2rpx; background:#c7c7c7; -webkit-transform-origin:bottom; transform-origin:bottom; -webkit-transform:scaleY(0.5); transform:scaleY(0.5);}
.tableView navigator .h1{padding-right:82rpx; position:relative; white-space:nowrap; text-overflow:ellipsis; background-repeat:no-repeat; background-image:url("data:image/svg+xml;charset=utf-8,%3Csvg%20viewBox%3D'0%200%2060%20120'%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%3E%3Cpath%20d%3D'm60%2061.5-38.25%2038.25-9.75-9.75%2029.25-28.5-29.25-28.5%209.75-9.75z'%20fill%3D'%23c7c7cc'%2F%3E%3C%2Fsvg%3E"); background-position:-webkit-calc(100% - 35rpx) center; background-position:calc(100% - 35rpx) center; background-size:23rpx 46rpx;}
.tableView navigator .h1.noPush{padding-right:0; background:none;}
.tableView .em{display:block; float:left; margin:16rpx 35rpx; width:70rpx; height:70rpx; text-decoration:none; background:no-repeat center center; background-size:cover;}
.tableView .h1 .em{margin-left:0}
.tableView .big, .tableView .small{display:block; margin:0; padding:0; font-weight:normal; font-size:28rpx; color:#bbb;}
.tableView .big{float:right; text-align:right; max-width:none; overflow:hidden; display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; -webkit-transform:scale(0.84); transform:scale(0.84); -webkit-transform-origin:right; transform-origin:right;}
.tableView .big .checkbox-app{margin-top:14rpx;}
.tableView .small{line-height:normal; -webkit-transform-origin:left; transform-origin:left;}
.tableView .big navigator{font-size:28rpx; color:#999;}
.navBar-hidden .tableView .section > .header{top:0;}

/*拖曳删除按钮*/
.delBtn{position:absolute; z-index:-1; top:0; right:0; text-align:center; width:175rpx; height:100%; overflow:hidden; color:#fff;}
.delBtn .del{width:100%; height:100%; background:#f00 url(http://bangfang.b0.upaiyun.com/uploadfiles/client/1/pic/2018/05/10/1851011094361107.png) no-repeat center center; background-size:49rpx 49rpx;}
.delBtnMask{position:absolute; z-index:100; top:0; left:0; width:100%; height:100%;}

.section > .tableView{position:relative; margin-bottom:30rpx;}
.section > .tableView:before, .section > .tableView:after{content:""; position:absolute; left:0; z-index:3; display:block; width:100%; height:2rpx; background:#c7c7c7; -webkit-transform:scaleY(0.5); transform:scaleY(0.5);}
.section > .tableView:before{top:0; -webkit-transform-origin:top; transform-origin:top;}
.section > .tableView:after{bottom:0; -webkit-transform-origin:bottom; transform-origin:bottom;}
.section > .header{position:relative; color:#333; width:100%; height:58rpx; line-height:58rpx; overflow:hidden; clear:both; text-align:left; white-space:nowrap; text-overflow:ellipsis; box-sizing:border-box; font-weight:bold; margin-top:35rpx;}

.tableView-light .section:before,
.tableView-light > .li:last-child:after, .tableView-light .h1:after,
.section > .tableView-light:before, .section > .tableView-light:after{background:#eaeaea;}

.tableView-noLine .section:before,
.tableView-noLine > .li:last-child:after, .tableView-noLine .h1:after,
.section > .tableView-noLine:before, .section > .tableView-noLine:after{display:none;}

.tableView-noGeLine > .li:last-child:after, .tableView-noGeLine .h1:after{display:none;}

/*字体缩小*/
.scale8{font-size:28rpx; -webkit-transform:scale(0.66); transform:scale(0.66);}
.scale8-left{font-size:28rpx; -webkit-transform:scale(0.66); transform:scale(0.66); -webkit-transform-origin:left; transform-origin:left;}
.scale8-right{font-size:28rpx; -webkit-transform:scale(0.66); transform:scale(0.66); -webkit-transform-origin:right; transform-origin:right;}
.scale9{font-size:28rpx; -webkit-transform:scale(0.75); transform:scale(0.75);}
.scale9-left{font-size:28rpx; -webkit-transform:scale(0.75); transform:scale(0.75); -webkit-transform-origin:left; transform-origin:left;}
.scale9-right{font-size:28rpx; -webkit-transform:scale(0.75); transform:scale(0.75); -webkit-transform-origin:right; transform-origin:right;}
.scale10{font-size:28rpx; -webkit-transform:scale(0.83); transform:scale(0.83);}
.scale10-left{font-size:28rpx; -webkit-transform:scale(0.83); transform:scale(0.83); -webkit-transform-origin:left; transform-origin:left;}
.scale10-right{font-size:28rpx; -webkit-transform:scale(0.83); transform:scale(0.83); -webkit-transform-origin:right; transform-origin:right;}
.scale11{font-size:28rpx; -webkit-transform:scale(0.91); transform:scale(0.91);}
.scale11-left{font-size:28rpx; -webkit-transform:scale(0.91); transform:scale(0.91); -webkit-transform-origin:left; transform-origin:left;}
.scale11-right{font-size:28rpx; -webkit-transform:scale(0.91); transform:scale(0.91); -webkit-transform-origin:right; transform-origin:right;}

/* 选项卡头 */
.switchView{position:relative; background:#fff; width:100%; height:82rpx; overflow:hidden;}
.switchView:after{content:""; display:block; position:absolute; left:0; bottom:0; z-index:99; background:#eaeaea; width:100%; height:2rpx; -webkit-transform:scaleY(0.5); transform:scaleY(0.5); -webkit-transform-origin:bottom; transform-origin:bottom;}
.switchView .row{width:100%; height:100%; overflow:auto; overflow-y:hidden; -webkit-overflow-scrolling:touch;}
.switchView .ul{height:100%; overflow:hidden; margin:0;}
.switchView .li{float:left; height:100%; padding:0 23rpx; box-sizing: border-box; white-space:nowrap; color:#bbb; font-size:28rpx;}
.switchView .li view, .switchView .li button{height:100%; line-height:82rpx; border-radius:0; background-color:transparent;}
.switchView .this{color:#ce0000; border-bottom:7rpx solid #ce0000;}

/* checkbox*/
.checkbox{display:block; text-align:left; padding-left:103rpx; box-sizing:border-box; width:auto; height:100%; background:url(http://bangfang.b0.upaiyun.com/uploadfiles/client/1/pic/2018/05/10/1851010341374315.png) no-repeat left center; background-size:auto 100%;}
.checkbox text{font-size:28rpx; color:#333; white-space:nowrap;}
.checkbox-x{background-image:url(http://bangfang.b0.upaiyun.com/uploadfiles/client/1/pic/2018/05/10/1851010491362978.png);}

/* 角标 */
.badge{position:relative;}
.badge .sub{display:block; position:absolute; z-index:10; top:11rpx; right:0; padding:0 7rpx; text-align:center; min-width:32rpx; height:32rpx; line-height:32rpx; border-radius:16rpx; background:#f00; box-sizing:border-box; color: #fff;}
.badge .sub:empty{display:none;}
.badge .sub .b{display:block; margin:0 auto; height:100%; font-weight:normal; color:#fff; font-size:28rpx; transform:scale(0.66);}

/*加载更多提示*/
.loadmore{height:117rpx; line-height:117rpx; text-align:center; clear:both;}
.loadmore:before{content:""; display:inline-block; vertical-align:middle; margin:0 11rpx; width:46rpx; height:46rpx; -webkit-animation:weuiLoading 1s steps(12, end) infinite; animation:weuiLoading 1s steps(12, end) infinite; background:url(data:image/svg+xml;base64,) no-repeat; background-size:100%;}
.loadmore:after{content:"正在加载"; display:inline-block; width:234rpx; vertical-align:middle; font-size:28rpx;}
.loadmore-nomore:before{display:none;}
.loadmore-nomore:after{content:"- END -"; color:#ccc;}

/* 提示框 */
.load-overlay{position:fixed; left:0; top:0; z-index:999998; width:100%; height:100%; overflow:hidden; text-align:center; opacity:0; -webkit-transition:opacity 200ms ease-out; transition:opacity 200ms ease-out; -webkit-backface-visibility:hidden; backface-visibility:hidden; -webkit-transform-style:preserve-3d; transform-style:preserve-3d;}
.load-overlay-in{opacity:1;}
.load-view{position:fixed; top:50%; left:50%; z-index:999999; min-width:281rpx; min-height:281rpx; overflow:hidden; background:rgba(0,0,0,0.6); border-radius:23rpx; opacity:0; -webkit-transform:scale(1.185) translate(-50%,-50%); transform:scale(1.185) translate(-50%,-50%); -webkit-transform-origin:center; transform-origin:center; -webkit-transition:all 200ms ease-out; transition:all 200ms ease-out; -webkit-backface-visibility:hidden; backface-visibility:hidden; -webkit-transform-style:preserve-3d; transform-style:preserve-3d;}
.load-view-in{-webkit-transform:scale(1) translate(-50%,-50%); transform:scale(1) translate(-50%,-50%); opacity:1;}
.load-view-out{-webkit-transform:scale(0.815) translate(-50%,-50%); transform:scale(0.815) translate(-50%,-50%); opacity:0;}
.load-view view{display:block; width:93rpx; height:93rpx; margin:0 auto; margin-top:80rpx; background:no-repeat center center; background-size:cover;}
.load-view text{display:block; color:#fff; line-height:42rpx; font-size:32rpx; padding:23rpx; text-align:center;}
.load-view text.text{font-size:32rpx;}
.load-view.nontext view{margin-top:106rpx !important;}
.load-view.nontext view + text{display:none !important;}
