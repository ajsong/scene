.index{padding:0 28rpx;}
.index .swiper{height:318rpx; background:#fff;}
.index .list .li{padding:35rpx 0; height:auto;}
.index .list .li > view{height:211rpx; background:no-repeat center center; background-size:cover;}

.index .list .li > view.type0{padding-right:306rpx;}
.index .list .li > view.type0 .pic{float:right; margin-right:-306rpx; width:283rpx; height:100%; background:no-repeat center center; background-size:cover;}
.index .list .li > view.type0 .span{display:block; width:100%; height:152rpx; overflow:hidden; font-size:37rpx; text-align:left;}
.index .list .li > view.type0 .bottom{height:25rpx; line-height:25rpx; text-align:left; font-size:23rpx; color:#999;}
.index .list .li > view.type0 .bottom .i{display:inline-block; margin-left:30rpx; vertical-align:top; width:25rpx; height:100%; background:no-repeat center center; background-size:cover;}
.index .list .li > view.type0 .bottom .look{background-image:url(data:image/png;base64,);}
.index .list .li > view.type0 .bottom .share{background-image:url(data:image/png;base64,); background-size:80% auto;}

.index .list .li > view.type5{position:relative; display:block; background:#fff; text-decoration:none; height:auto; color:#fff; text-align:left;}
.index .list .li > view.type5 .pic{position:relative; height:351rpx; border-radius:7rpx; overflow:hidden; background:no-repeat center center; background-size:cover;}
.index .list .li > view.type5 .pic:before{content:''; position:absolute; left:0; top:0; display:block; width:100%; height:100%; background:url("data:image/png;base64,") no-repeat center center; background-size:93rpx 93rpx;}
.index .list .li > view.type5 .pic .span{display:block; position:absolute; left:0; bottom:0; width:100%; height:auto; padding:18rpx; font-size:32rpx; font-weight:600; box-sizing:border-box; -webkit-border-bottom-left-radius:16rpx; border-bottom-left-radius:16rpx; -webkit-border-bottom-right-radius:16rpx; border-bottom-right-radius:16rpx; background:rgba(0,0,0,0.4); -webkit-backdrop-filter:blur(35rpx); backdrop-filter:blur(35rpx);}
.index .list .li > view.type5 .bottom{height:25rpx; line-height:25rpx; text-align:left; margin-top:11rpx; font-size:23rpx; color:#999;}
.index .list .li > view.type5 .bottom .i{display:inline-block; margin-left:30rpx; vertical-align:top; width:25rpx; height:100%; background:no-repeat center center; background-size:cover;}
.index .list .li > view.type5 .bottom .look{background-image:url(data:image/png;base64,);}
.index .list .li > view.type5 .bottom .share{background-image:url(data:image/png;base64,); background-size:80% auto;}

.list .li.shuffle > view{height:176rpx;}
.list .li.shuffle > view.style0 .bottom{margin-top:9rpx;}
.list .li.shuffle > view.style1{height:auto;}
.list .li.shuffle > view.style1{padding-right:0;}
.list .li.shuffle > view.style1 .span{height:auto;}
.list .li.shuffle > view.style1 .pics{height:128rpx; margin-top:11rpx;}
.list .li.shuffle > view.style1 .pics view{float:left; margin-left:23rpx; width:calc((100% - 23rpx * 2) / 3); height:100%; border-radius:7rpx; background:no-repeat center center; background-size:cover;}
.list .li.shuffle > view.style1 .pics view:first-child{margin-left:0;}