page{background-color:#eee;}

.index{padding:0 28rpx; padding-top:28rpx;}

.index .list .li{text-align:left; float:left; border-radius:9rpx; background-color:#fff; overflow:hidden; width:332rpx; height:433rpx; margin:0 28rpx 28rpx 0; box-shadow:0 0 18rpx rgba(0,0,0,0.3);}
.index .list .li:nth-child(2n){margin-right:0;}
.index .list .li view{position:relative; width:332rpx; height:332rpx; background:no-repeat center center; background-size:cover;}
.index .list .li view:before{content:''; position:absolute; left:0; top:0; display:block; width:100%; height:100%; background:url("data:image/png;base64,") no-repeat center center; background-size:93rpx 93rpx;}
.index .list .li text{display:block; margin-top:7rpx; font-size:30rpx; width:100%; height:93rpx; box-sizing:border-box; padding:0 11rpx; overflow:hidden;}