page{background:#e8dfd0;}

.result{margin:0 23rpx; height:auto; overflow:hidden;}
.result .h1{text-align:left; font-weight:700; font-size:51rpx; padding:0; margin:46rpx 0; height:auto;}
.result .click{display:block; background-color:transparent; color:#00a5e0; text-align:left; text-decoration:none; line-height:51rpx; height:51rpx; overflow:hidden; font-size:32rpx;}
.result .click text{color:#a2a9b6;}
.result .tips{color:#e36c09; font-weight:700; text-align:left; font-size:32rpx; height:117rpx;}
.result .tips view{display:inline-block; width:46rpx; height:67rpx; vertical-align:bottom; background:no-repeat center center; background-size:cover;}
.result .tips text{color:#00b0f0;}
.result .img{position:relative; margin-top:35rpx; width:100%; height:auto; overflow:hidden;}
.result .img view{display:none; text-decoration:none; position:absolute; z-index:1; left:0; bottom:0; width:100%; height:152rpx; font-size:32rpx; font-weight:600; line-height:196rpx; color:#666; background:repeat-x left top; background-size:auto 100%;}
