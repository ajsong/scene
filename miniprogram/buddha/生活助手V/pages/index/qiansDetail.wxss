@-webkit-keyframes jt-spin{0%{-webkit-transform:translateY(0);}50%{-webkit-transform:translateY(-24rpx);}100%{-webkit-transform:translateY(0);}}
.result .img image{width:100%; height:auto; vertical-align:bottom;}
.result .img-hidden{height:375rpx;}
.result .img-hidden view{display:block;}
.result .img-hidden::after{display:block;}
.result .btnView{padding:58rpx 0;}
.result .btn, .result .show{display:block; text-decoration:none; color:#fef4e9; margin:0 auto; background:#f78d1d; border:solid 2rpx #da7c0c; font-size:37rpx; width:562rpx; height:79rpx; line-height:75rpx; box-sizing:border-box; border-radius:39rpx; text-shadow:0 2rpx 2rpx rgba(0,0,0,0.3); box-shadow:0 2rpx 4rpx rgba(0,0,0,0.2);}
.result .banner, .result .footer{margin:0 28rpx; border-radius:11rpx; height:234rpx; background:no-repeat center center; background-size:cover;}

